<% layout("layouts/boilerplate")%>
    <h1>All Campgrounds</h1>
    <div><a href="/campgrounds/new">Add New Campground</a></div>
    <ol>
        <% for(let camp of campgrounds) {%>
            <li>
                <strong>
                    <a href="/campgrounds/<%= camp.id %>">
                        <%= camp.title %>
                    </a>
                </strong>, in <%= camp.location %>
            </li>
            <% } %>
    </ol>